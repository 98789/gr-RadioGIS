<?xml version="1.0"?>
<block>
  <name>data_submitter</name>
  <key>RadioGIS_data_submitter</key>
  <category>RadioGIS</category>
  <import>import RadioGIS</import>
  <make>RadioGIS.data_submitter($N, $gps)</make>

  <param>
    <name>Vector size</name>
    <key>N</key>
    <type>int</type>
  </param>

  <param>
    <name>GPS</name>
    <key>gps</key>
    <type>raw</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$N</vlen>
  </sink>

</block>
